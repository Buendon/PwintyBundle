{
  "name" : "buendon/pwinty-bundle",
  "description" : "A Symfony service in order to interact with the Pwinty API",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Buendon"
  }],
  "keywords" : [
    "Symfony", "Pwinty", "Print"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=5.5.9",
    "pwinty/php-pwinty" : "dev-api_2.3",
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "symfony/phpunit-bridge": "^3.0"
  },
  "autoload" : {
    "psr-0" : {
      "Buendon\\PwintyBundle" : ""
    }
  },
  "target-dir" : "Buendon/PwintyBundle",
  "repositories" : [{
    "type": "vcs",
    "url": "https://github.com/Buendon/php-pwinty"
  }],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0-dev"
    }
  }
}